<% var title = 'Experience' %>
<% include partials/header.ejs %>
<form method="post" action="/experience/<%= expid %>/<%= experiencename %>">
	<fieldset>
	<legend><%= experiencename %></legend>
	<ol>

	<% for(var i = 0; i < country.length; i++) { %>
		<div role="listitem" style="margin:1.5em;">

	<label for="<%= country[i].id %>"><%= country[i].texte %></label><br />
		<select name="<%= country[i].id %>" id="<%= country[i].id %>" <%= country[i].optionelle ? '' : 'required' %> >
		<option value></option>
	<% for(var reponse in country[i].reponses) { %>
		<option value="<%= reponse %>" <%= (country[i].prec_reponse == reponse) ? 'selected' : '' %> ><%= country[i].reponses[reponse] %></option>
	<% } %>
		</select><br />
	<%
		}
	%>



	
		</div>
	</ol>
	
	<input type="submit" value="Send" />
	</fieldset>
</form>
<% include partials/footer.ejs %>