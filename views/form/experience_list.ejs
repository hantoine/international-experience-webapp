<% var title = 'Form'; var current = 'form' %>
<% include ../partials/header.ejs %>
<div class="container p-5">
	<h4> Your abroad experiences :</h4>
	<div class="separator"></div>
	<div class="row">
		<div class="col-lg-9 col-md-12">

	<ul class="list-group">
		<% for(var i=0; i< explist.length; i++) { %>
			<a href="/form/<%= explist[i].id%>/"><li class="list-group-item"> <%= explist[i].name %></li></a>
			<div class="separator"></div>
			<% } %>
		</ul>
	</div>
	<div class="col-lg-3 col-md-12">
		<form action="/form/new">
			<input type="submit" class="btn btn-primary btn-lg" value="Add new experience" />
		</form>
		<div class="separator"></div>
		<form action="/form/logout">
			<input type="submit" class="btn btn-secondary btn-lg" value="Log out" />
		</form>
	</div>
	</div>
</div>
	<% include ../partials/footer.ejs %>
